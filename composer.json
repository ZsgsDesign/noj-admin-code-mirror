{
    "name": "zsgsdesign/noj-admin-code-mirror",
    "description": "code editor extension for laravel-admin, customized for NOJ.",
    "type": "library",
    "keywords": ["laravel-admin", "extension", "noj"],
    "homepage": "https://github.com/ZsgsDesign/noj-admin-code-mirror",
    "license": "MIT",
    "authors": [
        {
            "name": "John Zhang",
            "email": "zsgsdesign@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "encore/laravel-admin": "~1.6|~1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "ZsgsDesign\\CodeMirror\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ZsgsDesign\\CodeMirror\\CodeMirrorServiceProvider"
            ]
        }
    }
}
